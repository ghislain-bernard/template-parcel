{
  "author": "Ghislain Bernard <ghislain.bernard@gmail.com>",
  "dependencies": {
    "@parcel/config-default": "2.12.0",
    "@parcel/core": "2.12.0",
    "@parcel/optimizer-terser": "2.12.0",
    "@parcel/transformer-pug": "2.12.0",
    "@parcel/transformer-sass": "2.12.0",
    "@types/node": "22.3.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "by-node-env": "2.0.1",
    "eslint": "8.57.0",
    "htmlhint": "1.1.4",
    "parcel": "2.12.0",
    "rimraf": "5.0.10",
    "sharp": "0.31.3",
    "typescript": "5.5.4"
  },
  "description": "template",
  "license": "MIT",
  "name": "@gbernard/template-parcel",
  "packageManager": "pnpm@9.7.1",
  "scripts": {
    "build": "by-node-env",
    "build:development": "parcel build --no-autoinstall --no-optimize --no-source-maps",
    "build:production": "parcel build --no-autoinstall --no-source-maps",
    "build:test": "parcel build --no-autoinstall --no-source-maps",
    "clean": "rimraf .parcel-cache dist",
    "lint": "eslint --max-warnings=0 src/script.ts",
    "postbuild": "rimraf .parcel-cache",
    "prebuild": "rimraf .parcel-cache dist",
    "prestart": "rimraf .parcel-cache dist",
    "start": "by-node-env",
    "start:development": "parcel serve --no-autoinstall",
    "start:production": "parcel watch --no-autoinstall --no-hmr --no-source-maps",
    "version": "echo $npm_package_version"
  },
  "source": "src/index.pug",
  "version": "0.0.1"
}
